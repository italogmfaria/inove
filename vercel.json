{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/inove-painel/browser",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/apk/:filename",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=inove-app.apk"
        }
      ]
    }
  ]
}

